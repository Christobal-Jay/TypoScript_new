config {
	language = en
	locale_all = en_US.utf8
	sys_language_uid = 0
	sys_language_content = en
	sys_language_mode = strict
	linkVars = L(int)

	# Caching Headers for Varnish
	sendCacheHeaders = 1
	cache_period = 86400

	disablePrefixComment = 1

	disableImgBorderAttr = 1

	contentObjectExceptionHandler = 1

	noScaleUp = 1

#	baseURL = http://example.com/
	tx_realurl_enable = 1
	absRefPrefix = auto
}
